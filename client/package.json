{
  "name": "kauvery-video-consultation",
  "version": "1.0.0",
  "description": "Secure video consultation platform for Kauvery Hospital",
  "private": true,
  "main": "src/index.js",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@zegocloud/zego-uikit-prebuilt": "^2.15.1",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "server": "node ../server/server.js",
    "server:dev": "NODE_ENV=development node ../server/server.js",
    "server:prod": "NODE_ENV=production node ../server/server.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run start\"",
    "dev:server": "nodemon ../server/server.js",
    "start:prod": "npm run build && npm run server:prod",
    "lint": "eslint src/**/*.{js,jsx}",
    "lint:fix": "eslint src/**/*.{js,jsx} --fix",
    "security-check": "npm audit",
    "security-fix": "npm audit fix",
    "preinstall": "npm run security-check",
    "postinstall": "echo 'âœ… Dependencies installed successfully'"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2",
    "eslint": "^8.56.0"
  },
  "keywords": [
    "video-consultation",
    "telemedicine",
    "healthcare",
    "zego",
    "react",
    "express",
    "security"
  ],
  "author": "Kauvery Hospital",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kauvery-hospital/video-consultation.git"
  },
  "bugs": {
    "url": "https://github.com/kauvery-hospital/video-consultation/issues"
  },
  "homepage": "https://kauveryhospital.com/video-consultation",
  "config": {
    "unsafe-perm": false
  }
}
